<%- include ("../partials/header") -%>
<h1>User Profile</h1>
<div>
    <div>
        <h1>
            <%= user.firstName + " " + user.lastName %>
        </h1>
        <img src="<%= user.avatar %>" alt="user profile image">
    </div>
    <div>
            <ul>
                <% movie.forEach(function(movie){%>
        
                <li><a href="/movies/<%=movie._id%>"><%=movie.title%></a></li>
                
        <%})%>
        </ul>
    </div>
</div>
<%- include ("../partials/footer") -%>